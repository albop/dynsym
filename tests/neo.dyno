α <- 0.387 
β <- 0.96
γ <- 4.0
δ <- 0.1
ρ <- 0.9
kappa <- ρ/0.3

z[~] <- 0.0
k[~] <- ((1/β-(1-δ))/α)**(1/(α-1))
y[~] <- k[~]^α
i[~] <- δ*k[~]
c[~] <- y[~] - i[~]

e_z[t] <- N(0, 0.002)
e_y[t] <- N(0, 0.002)

e_z[0] <- 0.01
∀t, 0<=t<10: e_y[t] <- 0.01

z[t] = ρ*z[t-1] + e_z[t] + e_y[t]
y[t] = exp(z[t])*k[t-1]^α
k[t] = k[t-1]*(1-δ) + i[t]
c[t] = exp(z[t])*k[t-1]^α - i[t]
β*(c[t+1]/c[t])^(-γ)*(1-δ+α*y[t+1]/k[t]) = 1
